{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Extensions.Hosting" ],
    "MinimalLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Information"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Properties": {
      "Application": "Tic-Tac-Toe"
    }
  }
}
